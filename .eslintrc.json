{
  "extends": [
    "eslint:recommended"
  ],
  "parser": "babel-eslint",
  "env": {
    "node": true
  },
  "rules": {
    "no-multi-spaces": [ "error" ],
    "keyword-spacing": [ "error" ],
    "block-spacing": [ "error" ],
    "array-bracket-spacing": [ "error", "always" ],
    "object-curly-spacing": [ "error", "always" ],
    "arrow-spacing": [ "error" ],
    "wrap-iife": [ "error" ]
  }
}
